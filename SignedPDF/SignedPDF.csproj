<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="itext7" Version="7.1.15" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="ResourcePDF.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>ResourcePDF.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="ResourcePDF.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>ResourcePDF.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
